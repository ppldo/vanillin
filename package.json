{
  "name": "@ppldo/vanillin",
  "version": "0.0.0",
  "description": "Converting tool for migrate css modules code base to vanilla-extract.",
  "bin": {
    "vanillin": "src/cli.ts"
  },
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "vanilla-extract",
    "migrate",
    "migration"
  ],
  "scripts": {
    "test": "jest --coverage && eslint ."
  },
  "author": [
    "<rapthead@gmail.com>",
    "<hactr013@gmail.com>"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "peerDependencies": {
    "postcss": "^8.2.8"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "clean-publish": "^2.1.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.8",
    "simple-git-hooks": "^2.0.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.0.0"
  },
