        "build": "rm -rf dist && yarn test && tsc",
        "prepublishOnly": "yarn build"
    },
    "authors": [
        "<rapthead@gmail.com>",
        "<hactr013@gmail.com>",
        "<terma95@gmail.com>"
    ],
    "license": "MIT",
    "engines": {
        "node": ">=10.0.0"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/jest": "^26.0.23",
        "@types/lodash": "^4.14.170",
        "@vanilla-extract/css": "^0.5.1",
        "jest": "^26.6.3",
        "prettier": "^2.3.1",
        "ts-jest": "^26.5.6",
        "ts-node": "^10.0.0"
    },
    "dependencies": {
        "@types/command-line-args": "^5.0.0",
        "@types/command-line-usage": "^5.0.1",
        "camelcase": "^6.2.0",
        "command-line-args": "^5.1.1",
        "command-line-usage": "^6.1.1",
        "glob": "^7.1.7",
        "lodash": "^4.17.21",
        "postcss": "^8.2.8",
        "postcss-selector-parser": "^6.0.6",
        "postcss-value-parser": "^4.1.0",
        "typescript": "^4.2.4"
    },
    "jest": {
        "testEnvironment": "node",
        "coverageThreshold": {
