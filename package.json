{
  "name": "postcss-vanilla-extract-migrate",
  "version": "0.0.0",
  "description": "PostCSS plugin to migrate css code base to vanilla-extract",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "vanilla-extract",
    "migrate",
    "migration"
  ],
  "scripts": {
    "test": "jest --coverage && eslint ."
  },
  "author": "hactr <hactr013@gmail.com>",
  "license": "MIT",
  "repository": "hactrz/postcss-vanilla-extract-migrate",
  "engines": {
    "node": ">=10.0.0"
  },
  "peerDependencies": {
    "postcss": "^8.2.8"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "clean-publish": "^2.1.0",
    "eslint": "^7.21.0",
    "eslint-plugin-jest": "^24.2.1",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.8",
    "simple-git-hooks": "^2.0.2",
    "ts-jest": "^26.5.6"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:jest/recommended"
    ],
    "rules": {
      "jest/expect-expect": "off"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "dependencies": {
    "@vanilla-extract/css": "^0.5.1",
    "camelcase": "^6.2.0",
    "postcss-selector-parser": "^6.0.6",
    "typescript": "^4.2.4"
  }
}
