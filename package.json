{
    "name": "@ppldo/vanillin",
    "version": "0.1.3",
    "description": "Converting tool for migrate css modules code base to vanilla-extract.",
    "bin": {
        "vanillin": "dist/cli.js"
    },
    "repository": "https://github.com/ppldo/vanillin",
    "keywords": [
        "postcss",
        "css",
        "postcss-plugin",
        "vanilla-extract",
        "migrate",
